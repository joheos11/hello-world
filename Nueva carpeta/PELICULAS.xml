<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<item>
<title>DISKOKOSMIKO</title>
<link>$doregex[index]</link>

<regex>
  <name>index</name>
  <listrepeat><![CDATA[
        <title>[index.param2]</title>
        <link>$doregex[makelist]</link>
        ]]></listrepeat>
	<expres><link>[\s\S]*?</link> <title>[\s\S]*?</title></expres>
<page>https://raw.githubusercontent.com/joheos11/hello-world/master/Nueva%20carpeta/index_pelis.txt</page>
<cookieJar></cookieJar>
</regex>

<regex>
  <name>makelist</name>
  <listrepeat><![CDATA[
        <title> [makelist.param3]</title>
        <link>$doregex[getUrl]</link>
		<thumbnail>$doregex[poster]</thumbnail>
		<info>$doregex[plot]</info>
        ]]></listrepeat>
<expres><![CDATA[href="\/.*,(.*?),list.*\n.*\n.*\n.*href="\/([^"]*)".*?>(.*?)<]]></expres>
<page>[index.param1]/list,1,15</page>
<cookieJar></cookieJar>
</regex>

<regex>
<name>getUrl</name>
<expres>DownloadUrl":"([^"]+)<expres>
<page>http://diskokosmiko.mx/action/DownloadFile?location=fi&amp;f=[makelist.param1]</page>
<rawpost>fileId=[makelist.param1]&__RequestVerificationToken=$doregex[tok]</rawpost>
<referer>http://diskokosmiko.mx/[makelist.param2]</referer>
<connection>keep-alive</connection>
<accept>*/*</accept>
<X-Req>XMLHttpRequest</X-Req>
<cookieJar></cookieJar>
</regex>

<regex>
<name>tok</name>
<expres>DownloadFile.*RequestVerificationToken.*?value="([^"]+)<expres>
<page>http://diskokosmiko.mx/[makelist.param2]</page>
<connection>keep-alive</connection>
<cookieJar></cookieJar>
</regex>

<thumbnail>http://i.imgur.com/AWU5h6C.jpg</thumbnail>
</item>
